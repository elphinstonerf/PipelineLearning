pipeline {
    println("Hello")
    agent any
    stages {
        stage ('Buzz Buzz') {
            steps {
                print('Bees Buzz!')
            }
        }
        stage ('Bees Bees') {
            steps {
                echo 'Buzz, Bees, Buzz!'
                echo 'Bees Buzzing!'
            }
        }
    }
}